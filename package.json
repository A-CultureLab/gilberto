{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "adb devices && react-native run-android",
    "ios": "react-native run-ios",
    "android:release": "adb devices && react-native run-android --variant=release",
    "ios:device": "react-native run-ios --device \"남궁현의 iPhone\"",
    "ios:release": "react-native run-ios --configuration Release",
    "android:assembleRelease": "cd android && ./gradlew assembleRelease",
    "android:bundleRelease": "cd android && ./gradlew bundleRelease",
    "gql": "npm run download-schema && npm run codegen",
    "download-schema": "apollo service:download --endpoint=http://localhost:8080/graphql graphql-schema.json",
    "codegen": "npx apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --tagName=gql",
    "codepush": "npm run codepush:android && npm run codepush:ios",
    "codepush:android": "appcenter codepush release-react -a [key] -d Production",
    "codepush:ios": "appcenter codepush release-react -a [key] -d Production",
    "gradlew:clean": "cd android && ./gradlew clean && cd ..",
    "node:clean": "rm -rf node_modules && rm yarn.lock",
    "node:clean:force": "yarn cache clean",
    "android:clean": "rm -rf android/build && sudo rm -rf android/app/build && sudo rm -rf android/.gradle",
    "pod": "cd ios && pod install && cd ..",
    "pod:clean": "cd ios && rm -rf Pods && rm Podfile.lock && pod deintegrate && pod setup && pod repo update && cd ..",
    "pod:clean:force": "sudo rm -rf ~/Library/Caches/CocoaPods",
    "ios:clean": "cd ios && xcodebuild clean",
    "ios:clean:force": "rm -rf ~/Library/Developer/Xcode/DerivedData/*",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "oss": "license-checker --json --out ./src/assets/oss.json"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@gorhom/bottom-sheet": "^4.0.0-alpha.30",
    "@invertase/react-native-apple-authentication": "^2.1.2",
    "@react-native-clipboard/clipboard": "^1.8.2",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/push-notification-ios": "^1.8.0",
    "@react-native-firebase/app": "^12.7.2",
    "@react-native-firebase/auth": "^12.7.2",
    "@react-native-firebase/messaging": "^12.7.2",
    "@react-native-seoul/kakao-login": "^3.3.2",
    "@react-navigation/bottom-tabs": "^5.11.11",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "apollo-upload-client": "^16.0.0",
    "axios": "^0.21.1",
    "dayjs": "^1.10.5",
    "graphql": "^15.5.1",
    "iamport-react-native": "^2.0.0-rc.0",
    "react": "17.0.2",
    "react-hook-form": "^7.10.1",
    "react-native": "0.65.1",
    "react-native-device-info": "^8.1.3",
    "react-native-draggable-flatlist": "^2.6.2",
    "react-native-fast-image": "^8.3.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-hyperlink": "^0.0.19",
    "react-native-image-crop-picker": "^0.36.2",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^12.0.2",
    "react-native-nmap": "^0.0.66",
    "react-native-push-notification": "^7.4.0",
    "react-native-reanimated": "2.3.0-alpha.2",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.4.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "^12.1.1",
    "react-native-vector-icons": "^8.1.0",
    "react-native-webview": "^11.6.4",
    "subscriptions-transport-ws": "^0.9.19"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.64.5",
    "@types/react-native-push-notification": "^7.3.1",
    "@types/react-native-vector-icons": "^6.4.7",
    "@types/react-test-renderer": "^16.9.2",
    "apollo": "^2.33.4",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.0",
    "react-native-codegen": "^0.0.7",
    "react-native-flipper": "^0.61.0",
    "react-native-flipper-apollo-devtools": "^0.0.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}